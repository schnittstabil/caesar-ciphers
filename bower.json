{
  "name": "caesar-ciphers",
  "version": "0.1.2",
  "license": "MIT",
  "homepage": "https://github.com/schnittstabil/caesar-ciphers",
  "authors": [
    "Michael Mayer <michael@schnittstabil.de>"
  ],
  "description": "Multiple implementations of the Caesar Cipher.",
  "moduleType": [
    "amd",
    "globals"
  ],
  "keywords": [
    "ceasar",
    "chipher"
  ],
  "main": [
    "dist/*.js",
    "example/*.js",
    "example/index-amd.global-global.html",
    "example/index-global.html"
  ],
  "ignore": [
    ".*",
    "*~",
    "**/.*",
    "bin",
    "bower_components",
    "build",
    "example/main.js",
    "example/index-amd.global-amd.html",
    "example/index-amd.html",
    "example/index-requirejs.html",
    "lib",
    "node_modules",
    "test",
    "CONTRIBUTING.md",
    "Gruntfile.js",
    "index.js",
    "package.json"
  ]
}
