{
  "globals": {
    /* amdefine */
    "define":     true,
    /* MOCHA */
    "before":     true,
    "beforeEach": true,
    "describe":   true,
    "it":         true
  },
  "asi":          false,
  "bitwise":      true,
  "boss":         false,
  "camelcase":    true,
  "curly":        true,
  "debug":        false,
  "eqeqeq":       true,
  "eqnull":       false,
  "es5":          false,
  "esnext":       false,
  "evil":         false,
  "expr":         true,  // prevents warning "Expected an assignment or function call and instead saw an expression."
  "forin":        true,
  "funcscope":    false,
  "globalstrict": false,
  "immed":        true,
  "indent":       2,
  "iterator":     false,
  "lastsemic":    false,
  "latedef":      true,
  "laxbreak":     true,
  "laxcomma":     false,
  "loopfunc":     false,
  "maxerr":       20,
  "maxlen":       80,
  "multistr":     false,
  "newcap":       true,
  "newcap":       true,
  "noarg":        true,
  "node":         true,
  "noempty":      true,
  "nomen":        false,
  "onecase":      false,
  "onevar":       true,
  "passfail":     false,
  "plusplus":     false,
  "proto":        false,
  "quotmark":     "single",
  "regexdash":    false,
  "regexp":       false,
  "scripturl":    false,
  "shadow":       false,
  "smarttabs":    false,
  "strict":       false,
  "sub":          false,
  "supernew":     false,
  "trailing":     true,
  "undef":        true,
  "unused":       true,
  "white":        false
